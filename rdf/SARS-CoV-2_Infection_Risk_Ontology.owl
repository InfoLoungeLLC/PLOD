    <!-- http://plod.info/rdf/HighLevelCloseContact -->

    <owl:Class rdf:about="http://plod.info/rdf/HighLevelCloseContact">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/NET/c4dm/event.owl#place"/>
                                <owl:someValuesFrom>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://plod.info/rdf/afford"/>
                                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                                        <owl:onClass rdf:resource="http://plod.info/rdf/DropletReachableActivity"/>
                                    </owl:Restriction>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://plod.info/rdf/action"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://plod.info/rdf/DropletReachableActivity"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasDuration"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#numericDuration"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</xsd:minExclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://plod.info/rdf/situationOfActivity"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://plod.info/rdf/RiskActivitySituation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://plod.info/rdf/CloseContact"/>
        <rdfs:label xml:lang="en">High-level Close-contact</rdfs:label>
        <rdfs:label xml:lang="ja">高密接</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://plod.info/rdf/HighLevelCloseContact"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#equivalentClass"/>
        <owl:annotatedTarget>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/NET/c4dm/event.owl#place"/>
                                <owl:someValuesFrom>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://plod.info/rdf/afford"/>
                                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                                        <owl:onClass rdf:resource="http://plod.info/rdf/DropletReachableActivity"/>
                                    </owl:Restriction>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://plod.info/rdf/action"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://plod.info/rdf/DropletReachableActivity"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasDuration"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#numericDuration"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</xsd:minExclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://plod.info/rdf/situationOfActivity"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://plod.info/rdf/RiskActivitySituation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:annotatedTarget>
        <rdfs:comment>飛沫到達行動2個以上 and 15分以上 and 対面</rdfs:comment>
    </owl:Axiom>
    


