    <!-- http://plod.info/rdf/MediumLevelCloseContact -->

    <owl:Class rdf:about="http://plod.info/rdf/MediumLevelCloseContact">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/NET/c4dm/event.owl#place"/>
                                <owl:someValuesFrom>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://plod.info/rdf/afford"/>
                                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                        <owl:onClass rdf:resource="http://plod.info/rdf/DropletReachableActivity"/>
                                    </owl:Restriction>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://plod.info/rdf/action"/>
                                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                <owl:onClass rdf:resource="http://plod.info/rdf/DropletReachableActivity"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasDuration"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#numericDuration"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</xsd:minExclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://plod.info/rdf/situationOfActivity"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://plod.info/rdf/RiskActivitySituation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://plod.info/rdf/CloseContact"/>
        <rdfs:label xml:lang="en">Medium-level Close-contact</rdfs:label>
        <rdfs:label xml:lang="ja">中密接</rdfs:label>
    </owl:Class>
    


